{% extends '@project-ecommerce/base.html.twig' %}

{% block title %}ShippingMethod{% endblock %}

{% block body %}
    <h1>ShippingMethod</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ shipping_method.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ shipping_method.name }}</td>
            </tr>
            <tr>
                <th>PriceNet</th>
                <td>{{ shipping_method.priceNet }}</td>
            </tr>
            <tr>
                <th>PriceGross</th>
                <td>{{ shipping_method.priceGross }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app.admin.shipping-method.index') }}">back to list</a>

    <a href="{{ path('app.admin.shipping-method.edit', {'id': shipping_method.id}) }}">edit</a>

    {{ include('@project-ecommerce/administration/admin_shipping_method/_delete_form.html.twig') }}
{% endblock %}
